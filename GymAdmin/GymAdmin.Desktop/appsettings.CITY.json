{
  "SqliteConfig": {
    "NameFolder": "DataDb",
    "NameDataBase": "GymAdmin.db"
  },
  "BackupConfig": {
    "FolderPath": "Backups",
    "RetentionDays": 7,
    "AutoBackup": true
  },
  "SerilogConfig": {
    "LogFilePath": "Logs/log-.txt",
    "RetainedFileCountLimit": 7,
    "ConsoleOutputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
    "FileOutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:lj}{NewLine}{Exception}",
    "MinimumLevel": "Information"
  },
  "PathsConfig": {
    "Root": "%MyDocuments%/GymAdmin-City",
    "DataDir": "DataDb",
    "LogsDir": "Logs",
    "DbFile": "GymAdmin.db",
    "LogFilePattern": "log.txt",
    "SecretFile": "secret.json"
  },
  "InstallerConfig": {
    "FeedUrl": "https://pujolcity.github.io/GymAdmin-/update-test.xml"
  }
} 